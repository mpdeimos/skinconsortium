		overlay="player.button.Play"
		showOnStatus="Hold"
		songtickerMessage="Start Playback"
		tooltip="Play"
	/>
  <Wasabi:Button id="Stop" action="STOP" x="205" y="105" h="31" w="30"
		overlay="player.button.Stop"
  		songtickerMessage="Stop Playback"
		tooltip="Stop"
	/>
  <Wasabi:Button id="Next" action="NEXT" x="237" y="105" h="31" w="30" 
		overlay="player.button.Next"
  		songtickerMessage="Next Track"
		tooltip="Next"
	/>
  <Wasabi:Button id="Open" action="EJECT" x="269" y="105" h="31" w="30" 
		overlay="player.button.Open"
		songtickerMessage="Open File(s)"
		tooltip="Open File(s)"
	/>
