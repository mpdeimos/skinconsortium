		id="type" ticker="1"
		x="-62" y="1" relatx="1"
		w="60" h="20" relatw="1"
		align="left" valign="top"
		fontsize="12" bold="0" shadowx="1" shadowy="1" shadowcolor="255,255,255"
		color="color.Text" font="ttf"
	/>

	<layer x="-66" relatx="1" w="2" y="2" h="-2" relath="1" image="player.statusdiv"/>
 <text
  id="infoline.dummy"
  display="songinfo" default=""
  x="0" y="0"
  w="1" h="1"
  ghost="1" visible="0"
/>
	<text
		id="infoline" ticker="1"
		default=""
		x="2" y="1"
		w="-72" h="20" relatw="1"
		align="right" valign="top"
		fontsize="12" bold="0"
		color="color.Text" font="ttf" shadowx="1" shadowy="1" shadowcolor="255,255,255"
	/>

	<script id="status" param="normal" file="scripts/status.maki"/>

