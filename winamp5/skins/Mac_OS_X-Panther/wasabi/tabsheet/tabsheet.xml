<groupdef id="wasabi.tabsheet" >
  <Grid x="0" y="0" w="0" h="0" relatw="1" relath="1"
        topleft =  "wasabi.tabsheet.topleft"
        top =      "wasabi.tabsheet.top"
        topright = "wasabi.tabsheet.topright"
        bottomleft =  "wasabi.tabsheet.bottomleft"
        bottom =      "wasabi.tabsheet.bottom"
        bottomright = "wasabi.tabsheet.bottomright"
  />

	<layer x="12" y="8" h="-16" w="-24" relatw="1" relath="1" image="wasabi.tabsheet.middle" tile="1" />
	<layer x="0" y="8" h="-16" w="12" relath="1" image="wasabi.tabsheet.left" tile="1" />
	<layer x="-12" y="8" h="-16" w="12" relatx="1" relath="1" image="wasabi.tabsheet.right" tile="1" />
</groupdef>

<groupdef id="wasabi.tabsheet.button.selected.group" h="25" autowidthsource="text">

  <!--Layer x="5" y="0" w="-5" h="0" relatw="1" relath="1" renderbasetexture="1" /-->

  <Grid x="4" y="0" w="-4" h="0" relatw="1" relath="1"
        topleft =     "wasabi.tabsheet.button.active.topleft"
        topright =    "wasabi.tabsheet.button.active.topright"
        top =		 "wasabi.tabsheet.button.active.top"
        left =     "wasabi.tabsheet.button.active.left"
        right =    "wasabi.tabsheet.button.active.right"
        middle =   "wasabi.tabsheet.button.active.middle"
        bottomleft =     "wasabi.tabsheet.button.active.bottomleft"
        bottomright =    "wasabi.tabsheet.button.active.bottomright"
        bottom =   "wasabi.tabsheet.button.active.bottom"
  />

  <text
    id="text" ticker="1"
    default="Active Tab" 
    x="9" y="6"
    w="-14" relatw="1" h="14"
    font="tahoma" fontsize="14"
    color="color.Text"
   />


  <Layer id="mousetrap" x="4" y="0" w="-4" h="0" relatw="1" relath="1" />

</groupdef>

<groupdef id="wasabi.tabsheet.button.unselected.group" h="25" autowidthsource="text">

  <!--Layer x="5" y="0" w="-5" h="0" relatw="1" relath="1" renderbasetexture="1" /-->

  <Grid x="4" y="4" w="-4" h="-4" relatw="1" relath="1"
        topleft =     "wasabi.tabsheet.button.topleft"
        topright =    "wasabi.tabsheet.button.topright"
        top =   "wasabi.tabsheet.button.top"
        left =     "wasabi.tabsheet.button.left"
        right =    "wasabi.tabsheet.button.right"
        middle =   "wasabi.tabsheet.button.middle"
        bottomleft =     "wasabi.tabsheet.button.bottomleft"
        bottomright =    "wasabi.tabsheet.button.bottomright"
        bottom =   "wasabi.tabsheet.button.bottom"
  />

  <text
    id="text" ticker="1"
    default="Inactive Tab" 
    x="9" y="9"
    w="-14" relatw="1" h="14"
    font="tahoma" fontsize="14"
    color="color.Text"
   />

  <Layer id="mousetrap" x="4" y="0" w="-4" h="0" relatw="1" relath="1" />

</groupdef>

<groupdef id="wasabi.tabsheet.nobutton.group" h="25">
  <Layer x="0" y="-2" relaty="1" w="0" relatw="1" h="2"/>
</groupdef>

<groupdef id="wasabi.tabsheet.content" inherit_group="wasabi.tabsheet">
  <GuiObject id="content" x="2" y="2" w="-4" relatw="1" h="-2" relath="1" />
</groupdef>

<groupdef id="wasabi.tabsheet.content.noborder">
  <GuiObject id="content" x="0" y="0" w="0" relatw="1" h="0" relath="1" />
</groupdef>