<!-- ************************ COVER SEARCH ***************************** -->

<groupdef id="config.cover.note">
	<Wasabi:Text 
		id="description"
		text="The SearchEngine CANNOT view CoverArts stored in the ID3v2 Tag of mp3s yet.\nTo show a CoverArt you MUST have a jpg-file that agrees with one of the\ngiven filenames! For advanced Cover please install:\nJ.Richter's Cover and Tag Plugin (http://fire.prohosting.com/richter)"
		x="0" y="0"
  		w="0" h="0"
		relatw="1" relath="1"
	/>
</groupdef>

<groupdef id="config.cover">
	<Wasabi:Text 
		id="description"
		text="Titan uses an CoverSearch engine allowing you to show the CoverArt of any media.\nThe CoverArt can either be shown in the Right Drawer or in an own window!\nSearch order of the engine (provides only JPG's in the playitem directory):\n1. cover.jpg,    2. cdcover.jpg,    3. coverart.jpg,    4. folder.jpg,    5. front.jpg,\n6. |album|.jpg,    7. |artist| - |album|.jpg    8. |artist| - |title|.jpg\n9. |filename|.jpg    10. |title|.jpg"
		x="0" y="0"
  		w="0" h="0"
		relatw="1" relath="1"
	/>

	<Wasabi:Text 
		id="description"
		text="You can also search for an CoverArt in your Cover Directory (e.g. C:\Covers):"
		x="0" y="80"
  		w="0" h="30"
		relatw="1"
	/>

	<Wasabi:EditBox id="coverpath" x="0" y="100" relatw="1" w="0" h="20"/>

	<Wasabi:Text 
		id="description"
		text="Search order for the additional Cover Directory:\n1. |album|.jpg,    2. |artist| - |album|.jpg    3. |artist| - |title|.jpg\n4. |filename|.jpg    5. |title|.jpg"
		x="0" y="125"
  		w="0" h="50"
		relatw="1"
	/>

	<Wasabi:TitleBox id="des" x="0" y="170" w="0" h="-170" relatw="1" relath="1" title="Important Note" content="config.cover.note" />

</groupdef>


